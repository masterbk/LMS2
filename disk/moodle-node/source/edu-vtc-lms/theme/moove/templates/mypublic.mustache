    function togglePasswordVisibility(e) {
        let currentSpan = $(e);
        let passwordInput = currentSpan.closest('.password-input-container').find('input');
        const passwordToggle = currentSpan.find("i");

        if (passwordInput.attr('type') === "password") {
            passwordInput.attr('type','text');
            passwordToggle.removeClass("fa-eye");
            passwordToggle.addClass("fa-eye-slash");
        } else {
            passwordInput.attr('type','password');
            passwordToggle.removeClass("fa-eye-slash");
            passwordToggle.addClass("fa-eye");
        }
    }
    function getBack(e) {
        e.stopPropagation();
        e.currentTarget.children[0].style.display = 'none'
        $('.card-profile').css('display','block');
        const section = $('section .my_page ');
        section.css('display','none');
    }
    function changeTab(e,tabName) {
        const width = screen.width;
        const backButton = $('.my-page-hidden-arrow');
        if(width< 1024){
            $('.card-profile').css('display','none');
            backButton.css('display','block');
        }
        var i, x, tablinks;
        x = document.getElementsByClassName("my_page");
        for (i = 0; i < x.length; i++) {
            x[i].style.display = "none";
        }
        tablinks = document.getElementsByClassName("tablink");
        $(tablinks).find('path').css('fill','black');
        for (i = 0; i < x.length; i++) {
            tablinks[i].removeAttribute('style');
        }
        if (tabName.length < 5) {
            tabName = 'profile';
        }
        let curerntAlink = $(`.tablink.my-${tabName}`)[0];
        $(`.tablink.my-${tabName}`).find('path').css('fill','#1890FF');
        curerntAlink.style.color += "#1890FF";
        curerntAlink.style.borderColor  += "#1890FF";
        curerntAlink.style.background += "#F4F7FE";
        let functionName = `getMy${tabName}`;
        let func = eval(functionName);
        window.history.replaceState(null, null, `?tab=${tabName}`);
        document.getElementsByClassName(`my-${tabName}`)[1].style.display = "block";
        if (typeof func === "function") {
            func.call(this,event,tabName);
        }
    }
    function getMycourses(e,tabName,action = null,objCateId = null) {
        e.stopPropagation()
        let url = '';

        if(action == 'find') {
            let findValue = $('.find-my-courses').val();
            url = `{{ajaxurl}}?methodname=get_enrolled_courses&find=${findValue}`;
        } else if(action == 'sort') {
            let cateId = objCateId.value;
            url = `{{ajaxurl}}?methodname=get_enrolled_courses&category=${cateId}`;
        } else {
            url = '{{ajaxurl}}?methodname=get_enrolled_courses';
        }
        let classification = 'all'
        let limit = 0
        let offset = 0
        $.ajax({
            'url':`${url}&limit=${limit}&offset=${offset}&classification=${classification}`,
            success: function (html) {
                const displayBlock = $('.my_page.my-courses > div .dashboard-card-deck');
                displayBlock.html(html.html);
            },
            error: function (error) {
                console.log(error);
            }
        });
    }
    function getMynotification(e,tabName) {
        e.stopPropagation()
        $.ajax({
            'url':`{{ajaxurl}}?methodname=get_notifications`,
            success: function (html) {
                const displayBlock = $('.my_page.my-notification > div > div');
                displayBlock.html(html);
            },
            error: function (error) {
                console.log(`Error ${error}`);
            }
        });
    }
    function readedNotification(e,notificationId){
        e.stopPropagation();
        const parentElement = e.currentTarget;
        let callUrl = '';
        if(notificationId) {
            callUrl = `{{ajaxurl}}?methodname=markReadedNotification&notificationId=${notificationId}`;
        } else {
            callUrl = `{{ajaxurl}}?methodname=markReadedNotification`;
        }
        $.ajax({
            'url': callUrl,
            success: function (data) {
                if(parentElement.children[0] == 'DIV') {
                    parentElement.children[0].style.backgroundColor = "white"
                } else {
                    $('.content-item-container.notification').css("background-color","white")
                }
            },
            error: function (error) {
                console.log(`Error ${error}`);
                parentElement.children[0].style.backgroundColor = "red"
            }
        });
    }
    function getMyprofile() {
        // TODO nothing come up yet
        return true;
    }
    function getMycertificate(e,tabName) {
        e.stopPropagation()
        $.ajax({
            'url':`{{ajaxurl}}?methodname=get_certification`,
            success: function (html) {
                const displayBlock = $('.my_page.my-certificate');
                const iframe = document.createElement("iframe");
                iframe.src = html.html;

                iframe.addEventListener("load", () => {
                    const pdf = iframe.contentDocument.getElementById("pdf");
                    iframe.contentDocument.body.innerHTML = "";
                    iframe.contentDocument.body.appendChild(pdf)
                });

                displayBlock.html(iframe);
            },
            error: function (error) {
                console.log(`Error ${error}`);
            }
        });
        return true;
    }
    function getMystudy() {
        e.stopPropagation()
        $.ajax({
            'url':`{{ajaxurl}}?methodname=get_study`,
